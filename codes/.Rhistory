controlsToGeneralPerceptions <- function(ldf, name){
print("### General ##")
ndf  <- ldf
ndf$Q4 <- mapvalues(ndf$Q4, from = c("0 fornecer a informação solicitada.", "Fornecer a informação solicitada.", "Fornecer a informação solicitada apenas se o sistema exibir sua política de privacidade.", "Fornecer a informação solicitada apenas se você perceber que o sistema lhe dará algo em troca."), to =c("Fundamentalist", "Unconcerned", "Pragmatist", "Pragmatist"))
print(ndf)
privacyProfilesHitogramAndDemographics(ndf, name)
profilesSourcesOfConcerns(ndf, name)
computeAgreement(ndf, name)
}
preProcessAndHandle <- function(df, name){
df2 <- as.data.frame(sapply(df,gsub,pattern="Extremamente preocupado",replacement=4, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Muito preocupado",replacement=3, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Moderadamente preocupado",replacement=2, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Pouco preocupado",replacement=1, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Nada preocupado",replacement=0, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Não",replacement=0, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Sim",replacement=1, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Extremamente útil",replacement=4, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Muito útil",replacement=3, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Moderadamente útil",replacement=2, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Pouco útil",replacement=1, fixed = TRUE))
df2 <- as.data.frame(sapply(df2,gsub,pattern="Nada útil",replacement=0, fixed = TRUE))
ndf <- df2
controlsToGeneralPerceptions(ndf, name)
}
